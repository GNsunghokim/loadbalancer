connect 192.168.10.201 111
$vmid = create core: 1 memory: 0x1000000 storage: 0x220000 nic: mac: 0 dev: eth1 ibuf: 1024 obuf: 1024 iband: 1000000000 oband: 1000000000 pool: 0x400000 nic: mac: 0 dev: eth0 ibuf: 1024 obuf: 1024 iband: 1000000000 oband: 1000000000 pool: 0x400000 args: hello world
upload $vmid main
start $vmid


stdin $vmid 0 "server add -t 192.168.100.201:8080 0 -m nat"
stdin $vmid 0 "server add -t 192.168.100.201:8081 0 -m nat"
stdin $vmid 0 "server add -t 192.168.100.201:8082 0 -m nat"
stdin $vmid 0 "server add -t 192.168.100.201:8083 0 -m nat"
stdin $vmid 0 "server add -t 192.168.100.201:8084 0 -m nat"
stdin $vmid 0 "service add -t 192.168.100.101:80 1 -s r -out 192.168.100.253 0"
stdin $vmid 0 "service add -t 192.168.100.102:80 1 -s d -out 192.168.100.252 0"

stdin $vmid 0 "service list"
stdin $vmid 0 "server list"


