connect 192.168.100.254
$vmid = create core: 1 memory: 0x1000000 storage: 0x200000 nic: mac: 0 port: 0 ibuf: 1024 obuf: 1024 iband: 1000000000 oband: 1000000000 pool: 0x400000 nic: mac: 0 port: 1 ibuf: 1024 obuf: 1024 iband: 1000000000 oband: 1000000000 pool: 0x400000 args: hello world
upload $vmid main
start $vmid

stdin $vmid 0 "service add -u 192.168.100.200:80 0 -s rr -out 192.168.101.202 1"
stdin $vmid 0 "server add -u 192.168.101.201:8080 1 -m nat"
stdin $vmid 0 "server add -u 192.168.101.201:8081 1 -m nat"
stdin $vmid 0 "server add -u 192.168.101.201:8082 1 -m nat"
stdin $vmid 0 "server add -u 192.168.101.201:8083 1 -m nat"
stdin $vmid 0 "server add -u 192.168.101.201:8084 1 -m nat"
stdin $vmid 0 "service list"

